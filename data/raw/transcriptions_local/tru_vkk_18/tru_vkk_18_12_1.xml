        <lb n="03"/>п<hi rendition="#pal">р</hi>иш<hi rendition="#accent">о</hi>л уж<hi rendition="#accent">а</hi>к с<g ref="#underparenChar" place="below"/>въ<hi rendition="#pal">д</hi>ан<hi rendition="#accent">о</hi>въ ц<hi rendition="#accent">а</hi>рствъ <pause rendition="#pause2"/>
        <lb n="04"/><hi rendition="#pal">Л</hi><hi rendition="#accent">о</hi>к на <hi rendition="#pal">д</hi><hi rendition="#accent">е</hi>ушкину руб<hi rendition="#accent">а</hi>шку и <hi rendition="#pal">н</hi><hi rendition="#accent">е</hi>
        <lb n="05"/>а<hi rendition="#long">д</hi>аj<hi rendition="#accent">о</hi>т <pause rendition="#pause1"/> в<hi rendition="#accent">ы</hi>й<hi rendition="#pal">д</hi>еш зъ<g ref="#underparenChar" place="below"/><hi rendition="#pal">м</hi>а<hi rendition="#pal">н</hi><hi rendition="#accent">а</hi> з<hi rendition="#accent">а</hi>муж <pause rendition="#pause1"/>
        <lb n="06"/>тагд<hi rendition="#accent">а</hi> а<hi rendition="#long">д</hi><hi rendition="#accent">а</hi>м руб<hi rendition="#accent">а</hi>шку <pause rendition="#pause1"/> а<g ref="#underparenChar" place="below"/><hi rendition="#pal">н</hi>ь<g ref="#underparenChar" place="below"/>в<hi rendition="#accent">ы</hi>и<hi rendition="#pal">д</hi>ьш <pause rendition="#pause1"/>
        <lb n="07"/><hi rendition="#pal">н</hi>ь<g ref="#underparenChar" place="below"/>ад<hi rendition="#accent">а</hi>м <pause rendition="#pause2"/> а<g ref="#underparenChar" place="below"/>адн<hi rendition="#accent">а</hi> дачк<hi rendition="#accent">а</hi> б<hi rendition="#accent">ы</hi>ла у
        <lb n="08"/>п<hi rendition="#accent">а</hi>пы-м<hi rendition="#accent">а</hi>мы <pause rendition="#pause2"/> как ан<hi rendition="#accent">а</hi> а<hi rendition="#pal">д</hi><hi rendition="#accent">е</hi>лъ<hi rendition="#pal">с</hi>ь <pause rendition="#pause1"/> уж<hi rendition="#accent">а</hi>к 
        <lb n="09"/>захва<hi rendition="#pal">т</hi><hi rendition="#accent">и</hi>л и ута<hi rendition="#pal">щ</hi><hi rendition="#accent">и</hi>л в<g ref="#underparenChar" place="below"/>вад<hi rendition="#accent">у</hi> <pause rendition="#pause2"/>
        </p>

        <p rendition="#indent">
        <lb n="10"/>пр<hi rendition="#accent">о</hi>жи<hi rendition="#pal">л</hi>и т<hi rendition="#pal">р</hi>и г<hi rendition="#accent">о</hi>дъ а<hi rendition="#pal">н</hi><hi rendition="#accent">е</hi> т<hi rendition="#accent">а</hi>м <pause rendition="#pause2"/>
        <lb n="11"/>б<hi rendition="#accent">ы</hi>л м<hi rendition="#accent">а</hi><hi rendition="#pal">л</hi>ч'ик и <hi rendition="#pal">д</hi><hi rendition="#accent">е</hi>въ<hi rendition="#pal">ч</hi>къ <pause rendition="#pause2"/> п<hi rendition="#pal">р</hi>ишл<hi rendition="#accent">а</hi>
        <lb n="12"/>дам<hi rendition="#accent">о</hi>й к<g ref="#underparenChar" place="below"/>атц<hi rendition="#accent">у</hi> <pause rendition="#pause1"/> к м<hi rendition="#accent">а</hi><hi rendition="#pal">т</hi>о<hi rendition="#pal">р</hi>и <pause rendition="#pause1"/> к<g ref="#underparenChar" place="below"/>
        <lb n="13"/><g ref="#underparenChar" place="below"/>п<hi rendition="#accent">а</hi>пы-м<hi rendition="#accent">а</hi>мы <pause rendition="#pause2"/> А<hi rendition="#pal">т</hi><hi rendition="#accent">е</hi>ц-тъ радн<hi rendition="#accent">о</hi>й
        <lb n="14"/>спр<hi rendition="#accent">а</hi>шивъjьт <pause rendition="#pause1"/> А<g ref="#underparenChar" place="below"/>д<hi rendition="#accent">о</hi><hi rendition="#pal">ч</hi>ь<hi rendition="#pal">н</hi>къ <pause rendition="#pause1"/> к<hi rendition="#accent">а</hi>к дам<hi rendition="#accent">о</hi>й
        <lb n="15"/>папа<hi rendition="#pal">д</hi><hi rendition="#accent">о</hi>ш <pause rendition="#pause1"/> кагд<hi rendition="#accent">а</hi> зах<hi rendition="#accent">о</hi><hi rendition="#pal">ч</hi>ьш jит<hi rendition="#accent">и</hi><hi rendition="#pal">т</hi> <pause rendition="#pause2"/>
        <lb n="16"/>в<hi rendition="#accent">ы</hi>йду на<g ref="#underparenChar" place="below"/><hi rendition="#pal">б</hi><hi rendition="#accent">е</hi>рьк <pause rendition="#pause1"/> скаж<hi rendition="#accent">у</hi> <pause rendition="#pause1"/> уж<hi rendition="#accent">а</hi>к <pause rendition="#pause1"/> уж<hi rendition="#accent">а</hi>к <pause rendition="#pause1"/>
        <lb n="17"/>в<hi rendition="#accent">ы</hi>й<hi rendition="#pal">д</hi>и <pause rendition="#pause1"/> ва<hi rendition="#pal">з</hi><hi rendition="#pal">м</hi><hi rendition="#accent">и</hi> <hi rendition="#pal">м</hi>а<hi rendition="#pal">н</hi><hi rendition="#accent">а</hi> дам<hi rendition="#accent">о</hi>й <pause rendition="#pause1"/> <hi rendition="#accent">о</hi>н и
        <lb n="18"/>в<hi rendition="#accent">ы</hi>й<hi rendition="#pal">д</hi>ьт <pause rendition="#pause2"/>
        </p>

        <p rendition="#indent">
        <lb n="19"/>а<hi rendition="#pal">т</hi><hi rendition="#accent">е</hi>ц в<hi rendition="#accent">ы</hi><hi rendition="#pal">ш</hi>л н<hi rendition="#accent">а</hi><g ref="#underparenChar" place="below"/>бь<hi rendition="#pal">р</hi>ьк и гъва<hi rendition="#pal">р</hi><hi rendition="#accent">и</hi>т <pause rendition="#pause1"/>
        <lb n="20"/>уж<hi rendition="#accent">а</hi>к <pause rendition="#pause1"/> уж<hi rendition="#accent">а</hi>к <pause rendition="#pause1"/> в<hi rendition="#accent">ы</hi>й<hi rendition="#pal">д</hi><hi rendition="#accent">и</hi> <pause rendition="#pause1"/> ва<hi rendition="#pal">з</hi><hi rendition="#pal">м</hi>и <del type="pause" rendition="#overstrike"><pause rendition="#pause1"/></del> <hi rendition="#pal">м</hi>а<hi rendition="#pal">н</hi><hi rendition="#accent">а</hi> дам<hi rendition="#accent">о</hi>й <pause rendition="#pause2"/>
        <lb n="21"/><hi rendition="#accent">О</hi>н и в<hi rendition="#accent">ы</hi>шьл <pause rendition="#pause2"/> к<hi rendition="#accent">а</hi>к т<hi rendition="#accent">о</hi><hi rendition="#pal">л</hi>къ <hi rendition="#accent">о</hi>н в<hi rendition="#accent">ы</hi>шьл <pause rendition="#pause1"/>
        <lb n="22"/>а<hi rendition="#pal">т</hi><hi rendition="#accent">е</hi>ц jам<hi rendition="#accent">у</hi> гълав<hi rendition="#accent">у</hi> тъпар<hi rendition="#accent">о</hi>м и атруб<hi rendition="#accent">и</hi>л <pause rendition="#pause2"/>
        </p>

        <p rendition="#indent">
        <lb n="23"/>да<hi rendition="#pal">ч</hi>к<hi rendition="#accent">а</hi> в<hi rendition="#accent">ы</hi>шла <pause rendition="#pause1"/> ужак<hi rendition="#accent">а</hi> б<hi rendition="#accent">о</hi><hi rendition="#pal">л</hi>шь <hi rendition="#pal">н</hi><hi rendition="#accent">е</hi>ту <pause rendition="#pause1"/>
        <lb n="24"/>гълав<hi rendition="#accent">а</hi> атруб<hi rendition="#pal">л</hi>ьнъ <pause rendition="#pause1"/> <hi rendition="#pal">в</hi><hi rendition="#accent">и</hi><hi rendition="#pal">д</hi>а <pause rendition="#pause2"/>
        </p>

        <p rendition="#indent">
        <lb n="25"/>м<hi rendition="#accent">а</hi>мъ и гъвар<hi rendition="#accent">и</hi>т с<hi rendition="#accent">ы</hi>ну <pause rendition="#pause2"/> сын<hi rendition="#accent">о</hi><hi rendition="#pal">ч</hi>ьк <pause rendition="#pause1"/>

        <!-- PAGE 2 -->
        <pb n="13"/>
        <lb n="01"/>бы<hi rendition="#pal">т</hi> съла<hi rendition="#pal">в</hi><hi rendition="#accent">е</hi>jушкъj <pause rendition="#pause1"/> <hi rendition="#pal">л</hi>ат<hi rendition="#accent">а</hi>j нъд<g ref="#underparenChar" place="below"/>вад<hi rendition="#accent">о</hi>j и
        <lb n="02"/>нъпъ<hi rendition="#pal">м</hi>ин<hi rendition="#accent">а</hi>j св<hi rendition="#accent">о</hi>въ п<hi rendition="#accent">а</hi>пу <pause rendition="#pause2"/>
        </p>

        <p rendition="#indent">
        <lb n="03"/>а<g ref="#underparenChar" place="below"/>т<hi rendition="#accent">ы</hi> д<hi rendition="#accent">о</hi><hi rendition="#pal">ч</hi>ь<hi rendition="#pal">н</hi>къ <pause rendition="#pause1"/> быт л<hi rendition="#accent">а</hi>стъ<hi rendition="#pal">ч</hi>къj <pause rendition="#pause1"/>
        <lb n="04"/>т<hi rendition="#accent">о</hi>жь <hi rendition="#pal">л</hi>ат<hi rendition="#accent">а</hi>j нъд<g ref="#underparenChar" place="below"/>вад<hi rendition="#accent">о</hi>j <pause rendition="#pause1"/> а я б<hi rendition="#accent">у</hi>ду
        <lb n="05"/>кук<hi rendition="#accent">у</hi>шь<hi rendition="#pal">ч</hi>къ<del type="accent" rendition="#overstrike"><g ref="#accentChar" place="above"/></del>j <pause rendition="#pause1"/> кук<hi rendition="#accent">у</hi>шкъj б<hi rendition="#accent">у</hi>ду кукав<hi rendition="#accent">а</hi><hi rendition="#pal">т</hi> <pause rendition="#pause1"/>
        <lb n="07"/>па<g ref="#underparenChar" place="below"/>св<hi rendition="#accent">о</hi>му м<hi rendition="#accent">у</hi>жу фспа<hi rendition="#pal">м</hi>ин<hi rendition="#accent">а</hi><hi rendition="#pal">т</hi> <pause rendition="#pause2"/>
        </p>

        <p rendition="#indent">
        <lb n="08"/>И ръз<hi rendition="#pal">л</hi>и<hi rendition="#sup" xml:lang="ru">е</hi>т<hi rendition="#accent">е</hi><hi rendition="#pal">л</hi>ис па<g ref="#underparenChar" place="below"/><hi rendition="#pal">б</hi><hi rendition="#accent">е</hi>лу <hi rendition="#pal">н</hi><hi rendition="#accent">е</hi>бу <pause rendition="#pause2"/>
