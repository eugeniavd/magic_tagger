      <!-- PAGE 1 -->
        <pb n="12"/>
        <p rendition="#indent">
        <lb n="01"/>паш<hi rendition="#pal">л</hi><hi rendition="#accent">и</hi> <hi rendition="#pal">д</hi><hi rendition="#accent">е</hi>уш<hi rendition="#pal">к</hi>ь куп<hi rendition="#accent">а</hi><hi rendition="#long">ц</hi>а <pause rendition="#pause2"/>
